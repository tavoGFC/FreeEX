        <activity android:name=".conexionBluetooth.ShowDevices" >
        </activity>
        <activity
            android:name=".Ingresar"
            android:label="@string/title_activity_ingresar" >
        </activity>
        <activity
            android:name=".EnviarMensajes"
            android:label="@string/title_activity_enviar_mensajes" >
        </activity>
        <activity
            android:name=".ConversacionUnoAUno"
            android:label="@string/title_activity_conversacion_uno_auno" >
        </activity>
    </application>

</manifest>
